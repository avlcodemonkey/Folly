@using Folly.Resources;
@model Folly.Models.Help

<content>
    <breadcrumb>
        <breadcrumb-item label="@Core.ToggleHelp" controller="Account" action="ToggleContextHelp" is-active="true" />
    </breadcrumb>

    <partial name="_Alert" />

    <lit-alert type="Success" no-dismiss>@(Model.IsEnabled ? Core.HelpEnabled : Core.HelpDisabled)</lit-alert>
</content>
