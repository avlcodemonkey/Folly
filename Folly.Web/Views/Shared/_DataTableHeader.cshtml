@using Folly.Resources

<div class="row">
    <div class="col col-10-md">
        <input type="text" name="tableSearch" placeholder="@Core.Search" class="col-6 col-4-md" data-table-search disabled>
    </div>
    <div class="col col-2-md is-right is-vertical-align">
        <div class="is-hidden" data-table-row-numbers>
            <span data-table-start-row></span><span> @Core.TableTo </span><span data-table-end-row></span><span> @Core.TableOf </span><span data-table-filtered-rows></span>
        </div>
    </div>
</div>
