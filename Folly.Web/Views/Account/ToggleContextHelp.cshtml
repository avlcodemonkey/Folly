@using Folly.Resources;
@model Folly.Models.Help

<content>
    <breadcrumb>
        <breadcrumb-item label="@Core.Help" controller="Account" action="ToggleContextHelp" active="true" />
    </breadcrumb>

    <partial name="_Alerts" />

    <partial name="_Alert" model="new Alert { Content = Model.IsEnabled ? Core.HelpEnabled : Core.HelpDisabled, CanDismiss = false }" />
</content>
