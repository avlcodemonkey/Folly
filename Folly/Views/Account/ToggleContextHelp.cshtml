@using Folly.Resources;
@model Folly.Models.Help

<content>
    <breadcrumb>
        <breadcrumb-item label="@Core.ToggleHelp" controller="Account" action="ToggleContextHelp" is-active="true" />
    </breadcrumb>

    <partial name="_Alert" />

    <partial name="_AlpineAlert" model="new AlpineAlert { Content = Model.IsEnabled ? Core.HelpEnabled : Core.HelpDisabled, CanDismiss = false }" />
</content>
